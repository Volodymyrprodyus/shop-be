{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "serverless deploy",
    "test": "jest",
    "docs": "redoc-cli serve docs/openapi/openapi.yml",
    "docs:build": "redocly build-docs --output=./docs/openapi/openapi.html",
    "docs:preview": "redocly preview-docs -p 8888",
    "ts-to-yaml-convert": "npx ts-node ts-to-yaml.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.6.4",
    "@middy/http-json-body-parser": "^3.4.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.2.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "jest": "^29.7.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "yaml": "^2.3.2"
  },
  "author": "Volodymyr Prodyus",
  "license": "MIT"
}
